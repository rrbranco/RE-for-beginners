.LC1:
	.string	"a=%d; b=%d; c=%d"
f2:
; сохранить FP и LR в стековом фрейме:
	stp	x29, x30, [sp, -16]!
; установить стековый фрейм (FP=SP):
	add	x29, sp, 0
	adrp	x0, .LC1
	add	x0, x0, :lo12:.LC1
	mov	w1, 1
	mov	w2, 2
	mov	w3, 3
	bl	printf
	mov	w0, 0
; восстановить FP и LR
	ldp	x29, x30, [sp], 16
	ret
